{
  "group": "Back-Channel Logout",
  "usage": {
    "back": true,
    "discover": true
  },
  "desc": "Checks Back Channel related openid-configuration discovery information",
  "sequence": [
    {
      "Webfinger": {
        "set_webfinger_resource": null
      }
    },
    {
      "Discovery": {
        "set_discovery_issuer": null
      }
    }
  ],
  "MTI": [],
  "assert": {
    "verify-op-endpoints-use-https": null,
    "verify-required-claims-true": {
      "ProviderConfigurationResponse": [
        "backchannel_logout_supported",
        "backchannel_logout_session_supported"
      ]
    }
  },
  "reference": [
    "https://openid.net/specs/openid-connect-backchannel-1_0.html#BCSupport"
  ]
}
